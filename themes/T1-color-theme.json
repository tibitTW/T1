        "sideBarSectionHeader.background": "#1d5c61",
        "textLink.foreground": "#8dcbcf",
        "dropdown.background": "#132d30",
        "list.focusBackground": "#102a2c",
        "menu.background": "#174f53",
        "menu.selectionBackground": "#0a2f31",
        "button.background": "#1e666b",
        "activityBar.background": "#0c2425",
        "panel.background": "#142d2e",
        "tab.activeBackground": "#204346",
        "tab.inactiveBackground": "#0e3133",
        "tab.activeBorderTop": "#00bbc9",
        "editorGroup.border": "#00bbc9",
        "editorGroup.dropBackground": "#5ecbd42c",
        "editorGroupHeader.tabsBackground": "#0e3133",
        "editor.background": "#204346",
        "editor.foreground": "#c5e4e4",
        "editorLineNumber.foreground": "#8dcbcf80",
        "editorLineNumber.activeForeground": "#aeebf0",
        "editorCursor.background": "#154144",
        "editorCursor.foreground": "#b4e1e6",
        "editor.selectionBackground": "#44a0a873",
        "editor.findMatchBackground": "#00000000",
        "editor.findMatchBorder": "#8dcbcf",
        "editor.findMatchHighlightBackground": "#00000000",
        "editor.findMatchHighlightBorder": "#8dcbcf91",
        "editor.lineHighlightBackground": "#2a565a7c",
        "editor.lineHighlightBorder": "#52858a",
        "editorWidget.background": "#215052",
        "editorWidget.border": "#8fd6da",
        "activityBarBadge.background": "#00426e",
        "input.background": "#132d30",
        "list.hoverBackground": "#226064",
        "list.activeSelectionBackground": "#226064",
        "list.inactiveSelectionBackground": "#1d5053",
        "terminal.background": "#142d2e"
    },
    "tokenColors": [
        {
            "name": "String",
            "scope": ["string"],
            "settings": {
                "foreground": "#00ffff"
            }
        },
        {
            "name": "Number",
            "scope": ["constant.numeric"],
            "settings": {
                "foreground": "#4aa8ff"
            }
        },
        {
            "name": "Define",
            "scope": ["storage.type.class", "meta.fuction"],
            "settings": {
                "foreground": "#ce9494",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Function",
            "scope": ["entity.name.function", "entity.name.class"],
            "settings": {
                "foreground": "#FF0000",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Function Argument",
            "scope": ["variable.parameter"],
            "settings": {
                "foreground": "#4aa8ff",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Keyword",
            "scope": ["keyword.control"],
            "settings": {
                "foreground": "#ff3287",
                "fontStyle": "bold "
            }
        }
    ]
}
